function endGame(p: player={_none}):
	if {timer} is set:
		removeArmor({tagger})
		delete {tagger}
		loop {players::*}:
			leaveGame(loop-value)
		delete {players::*}
		delete {queue::*}
		delete {timer}
		broadcast ""
		broadcast "  &eThe tag game has ended!"
		broadcast ""
		send "&eYou have ended the game!" to {_p}
	else:
		send "&cThere is no tag game running!" to {_p}